for _ in range(int(input())):
    n, count = int(input()), 0

    if n%10 == 0:  
        print('0')
        continue
        
    for i in range(10):
        n *= 2
        count += 1
        if n%10 == 0:  
            print(count)
            break
    else:  print('-1')
    
# ---- Alernate solution ---- #

for _ in range(int(input())):
    n=int(input())
    if n%10==0:
        print(0)
    elif n%5==0:
        print(1)
    else:
        print(-1)
