for _ in range(int(input())):
    k = int(input())
    for i in range(8):
        for j in range(8):
            if i==j and i==0: 
                print('O', end='')
                k -= 1
            elif k:
                print('.', end='')
                k -= 1
            else:
                print('X', end='')
        print()
